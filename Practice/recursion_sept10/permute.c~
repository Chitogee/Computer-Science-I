# include <stdio.h>
#include <stdlib.h>
void cooco(char *tmp ){

    char b[] = "\e[38;5;26m";
    char c[] = "\e[0m";
    char * original = b;
    char * add = tmp ;
    while(*original)
        original++;

    while(*add)
    {
        *original = *add;
        add++;
        original++;
    }
    //add = c ;
    //while(*add)
    //{
    //    *original = *add;
    //    add++;
    //    original++;
   // }
    *original = '\0';
}

void concatenate_string(char *original, char *add)
{
       while(*original)
                 original++;

          while(*add)
                 {
                           *original = *add;
                                 add++;
                                       original++;
                                          }
             *original = '\0';
             *add = *original ;
}




/* Driver program to test above functions */
